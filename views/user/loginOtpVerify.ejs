								<span id="countdown"></span>
							</div>

                            <div class="col-md-12 form-group">
								<input type="tel" class="form-control"  name="otp" placeholder="OTP" onfocus="this.placeholder = ''" onblur="this.placeholder = 'OTP'">
							</div>
								
							<div class="col-md-12 form-group mb-5">
								<button type="submit" value="submit" class="primary-btn">Login</button>
							</div>
                            <% if(locals.verificationMessage){ %>
								<div id="msg" class="alert alert-danger"><%=locals.verificationMessage%></div>
							 <% } %>  
							 <% setTimeout %>
							
						</form>
						
						
					</div>
				</div>
				<div class="col-md-2"></div>
			</div>
		</div>
	</section>
